}
.homie-award image{
  width: 645rpx;
  height: 27rpx;
}
.enter-btn{
  width: 595rpx;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  margin: 37rpx auto;
  background: #eefff5;
  border-radius: 50rpx;
  font-size: 36rpx;
  color: #46bd71;
  font-weight: bolder;
  box-shadow: 0 0 30rpx rgba(0,0,0,.3);
}
.dialog-box{
  position: fixed;
  width: 100%;
  height: 100%;
  top:0;
  left: 0;
  background:rgba(0,0,0,.3);
  z-index: 10;
}
.dialog-box .dialog-content{
  width: 600rpx;
  /*height: 403rpx;*/
  background: #fff;
  border-radius: 10rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%)
}
.dialog-box .dialog-content .close{
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: -20rpx;
  top: -20rpx;
}
.dialog-box .dialog-content .close image{
  width: 100%;
  height: 100%;
}
.dialog-box .dialog-content .dialog-image image{
  width: 100%;
}
.dialog-box .dialog-content .dialog-image{
  width: 450rpx;
  height: 550rpx;
  overflow-y: scroll;
  margin: 40rpx auto;
  text-align: center;
  font-size: 28rpx;
  color: #5b5b5b;
}
.dialog-box .dialog-content .qqyy-pic{
  width: 520rpx;
  height: 550rpx;
  margin: 40rpx;
}
.dialog-box .dialog-content .dialog-text text{
  font-size: 22rpx;
  color: #46bd71;
  word-break: break-all;
  display: block;
  margin-bottom: 10rpx;
}
.dialog-box .dialog-content .dialog-text .title{
  font-size: 22rpx;
  margin-bottom: 20rpx;
  color: #5d5d5d;
  word-break: break-all;
}
.dialog-box .dialog-content .dialog-text{
  width:90%;
  margin: 0 auto;
  text-align: center;
  padding: 90rpx 0;
  font-size: 28rpx;
  color: #5b5b5b;
}



.warranty-content{
  width: 100%;
  text-align: left;
  margin: 0 auto;
  max-height: 600rpx;
  overflow-y: scroll;
}
.warranty-content .sm-title{
  font-size: 30rpx;
  color: #000;
  margin-bottom: 30rpx;
  text-align: center;
}
.warranty-content .rule-box .rule-tit{
  width: 100%;
