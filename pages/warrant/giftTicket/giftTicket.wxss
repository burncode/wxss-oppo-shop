}
.warranty-content .rule-box{
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  margin-bottom: 10rpx;
  flex-wrap: wrap;
}
.warranty-content .rule-box .serial-num{
  width: 35rpx;
  height: 35rpx;
  background: #ffca13;
  text-align: center;
  line-height: 35rpx;
  color: #fff;
  border-radius: 100%;
  font-size: 26rpx;
  margin-right: 25rpx;

}
.warranty-content .rule-box .rule-text-1{
  width: 95%;
  padding-left: 5%;
  font-size: 24rpx;
  color: #8b8b8b;
  line-height: 35rpx;
  margin-bottom: 5rpx;
}
.warranty-content .rule-box .rule-text{
  flex:1;
  font-size: 24rpx;
  color: #8b8b8b;
  line-height: 35rpx;
}
.aqiyi-slogan{
  color: #00bd7a !important;
  font-size: 26rpx;
}
.guize-btn{
  width: 150rpx;
  height:50rpx;
  font-size: 24rpx;
  text-align: center;
  color: #fff;
  position: absolute;
  z-index: 999;
  left: 0;
  top: 40rpx;
  background: #41a96f;
  line-height: 50rpx;
  border-radius: 0 50rpx 50rpx 0;
}