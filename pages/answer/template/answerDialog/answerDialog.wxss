.dialog-module{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 999;
}
.dialog-module .musk{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,.5);
}
.dialog-module .dialog-box{
  width: 600rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  background: #fff;
  border-radius: 15rpx;
  padding: 90rpx 0;
  text-align: center;
}
.dialog-module .dialog-box .dialog-btn-box .normal .text{
  background: -webkit-linear-gradient(right, #ee7435, #dc3065);
  background: -o-linear-gradient(right, #ee7435, #dc3065);
  background: linear-gradient(to left, #ee7435, #dc3065);     /* 背景色渐变 */
  -webkit-background-clip: text;         /* 规定背景的划分区域 */
  -webkit-text-fill-color: transparent;  /* 防止字体颜色覆盖 */
}
.dialog-module .dialog-box .dialog-btn-box .normal{
  font-size: 32rpx;
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  text-align:center;
  border-radius:50rpx; 
  text-align:center;
  height: 84rpx;
  line-height: 84rpx;
  margin-left: 15rpx;
}
.dialog-module .dialog-box .dialog-btn-box .common{
  text-align:center;
  background: -webkit-linear-gradient(right, #ee7435 , #dc3065);     /* 背景色渐变 */
  border-radius:50rpx; 
  font-size: 32rpx;
  color: #fff;
  height: 84rpx;
  line-height: 84rpx;
}
